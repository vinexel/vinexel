{
    "name": "vision-serve/vision",
    "description": "A fullstack multi project web framework with Hierarchical Model View Controller (HMVC) Multi Project, hybrid Golang restapi and PHP rendering Framework",
    "keywords": ["Framework", "PHP", "Golang", "MVC", "hybrid Golang and PHP Framework", "Vision", "PHP Vision Framework", "Golang Vision Framework", "Vision Indonesia", "PHP Vision", "Golang Vision"],
    "type": "project",
    "license": "MIT",
    "homepage": "https://visioniconic.com",
    "version": "1.0.0",

    "support": {
        "email": "help@visioniconic.com",
        "source": "https://github.com/visionframework/",
        "docs": "https://visioniconic.com/docs"
    },

    "authors": [
        {
            "name": "Elwira Perdana",
            "email": "elwira@visioniconic.com",
            "homepage": "https://github.com/visionframework/",
            "role": "developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "System\\": "system/",
            "Vision\\": "system/framework/",
            "Deeper\\": "system/vendor/vision/fragment/kit/deeper/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Vision\\Tests\\": "tests/"
        }
    },

    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "vendor-dir": "system/vendor/plugins"
    },

    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "twig/twig": "^3.18"
    }
}
