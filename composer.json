{
    "name": "vinexel/vinexel",
    "description": "A fullstack multi project web framework with Hierarchical Model View Controller (HMVC) Multi Project, hybrid Golang restapi and PHP rendering Framework",
    "keywords": ["Framework", "PHP", "Golang", "HMVC", "hybrid Golang and PHP Framework", "Vinexel", "PHP Vinexel Framework", "Golang Vinexel Framework", "Vinexel Indonesia", "PHP Vinexel", "Golang Vinexel"],
    "type": "project",
    "license": "MIT",
    "homepage": "https://vinexel.com",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true,

    "support": {
        "email": "help@visioniconic.com",
        "source": "https://github.com/visionframework/vision",
        "docs": "https://vinexel.com/docs"
    },

    "authors": [
        {
            "name": "Elwira Perdana",
            "email": "elwira@visioniconic.com",
            "homepage": "https://github.com/visionframework/",
            "role": "developer"
        }
    ],

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vinexel/vision-serve"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "System\\": "system/",
            "Vision\\": "system/framework/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Vision\\Tests\\": "tests/"
        }
    },

    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "vendor-dir": "system/vendor/plugins"
    },

    "require": {
        "php": "^8.0",
        "vinexel/vision-serve": "1.0.0"
    }
}
