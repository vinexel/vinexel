              <section id="section-2">
                <h2 class="section-title-2">Stack Trace <a data-scroll="" href="#section-2" class="anchor" data-toggle="tooltip" data-placement="top" title="Follow"><i class="icon-hash"></i></a></h2>
                <p>To switch directories, type <kbd class="kbd">cd</kbd> followed by the name of the directory. To edit settings, press <kbd class="kbd">ctrl + ,</kbd></p>
                <div class="row">
                  <div class="col col-xxl-8">
                    <div class="table-responsive-md">
                      <pre>
                      <table class="table table-striped boxed">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">File</th>
                            <th scope="col">Line</th>
                            <th scope="col">Function</th>
                        </tr>
                    </thead>
                    <tbody>
                        <?php
                        // Cek apakah trace adalah string atau array
                        $trace = $errorDetails['trace'];

                        // Jika trace adalah string, mungkin dalam bentuk yang bisa dipisah oleh garis baru
                        if (is_string($trace)) {
                          $trace = explode("\n", $trace); // Pisahkan string berdasarkan garis baru
                        }

                        // Definisikan path root framework yang ingin kita hilangkan
                        $frameworkRoot = VISION_START; // Sesuaikan dengan root dari framework Anda

                        // Pastikan trace sekarang adalah array dan lakukan iterasi
                        if (is_array($trace)) {
                          foreach ($trace as $index => $item) {
                            // Cek apakah item berupa string (jika ini hasil dari explode)
                            if (is_string($item)) {
                              // Gunakan regex untuk mengekstrak file, line, dan function
                              preg_match('/#\d+ (.*)\((\d+)\): (.*)/', $item, $matches);

                              // Ambil nama file dari regex, dan hilangkan bagian root path
                              $file = isset($matches[1]) ? htmlspecialchars(str_replace($frameworkRoot, '', $matches[1])) : '(unknown file)';
                              $line = isset($matches[2]) ? htmlspecialchars($matches[2]) : '(unknown line)';
                              $function = isset($matches[3]) ? htmlspecialchars($matches[3]) : '(unknown function)';
                            } else {
                              // Jika item adalah array atau object, proses seperti biasa
                              $file = isset($item['file']) ? htmlspecialchars(str_replace($frameworkRoot, '', $item['file'])) : '(unknown file)';
                              $line = isset($item['line']) ? htmlspecialchars($item['line']) : '(unknown line)';
                              $function = isset($item['function']) ? htmlspecialchars($item['function']) : '(unknown function)';
                            }
                        ?>
                                <tr>
                                    <th scope="row"><?php echo $index + 1; ?></th>
                                    <td><?php echo $file; ?></td>
                                    <td><?php echo $line; ?></td>
                                    <td><?php echo $function; ?></td>
                                </tr>
                        <?php
                          }
                        } else {
                          echo '<tr><td colspan="4">No stack trace available</td></tr>';
                        }
                        ?>
                    </tbody>
                </table>
                      </pre>
